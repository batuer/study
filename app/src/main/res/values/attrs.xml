<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="RightArrowTv">
    <attr name="text" format="string" />
    <attr name="right_arrow" format="reference" />
    <attr name="text_color" format="color" />
    <attr name="text_size" format="dimension" />

  </declare-styleable>
  <declare-styleable name="FormLayout">
    <attr name="column_weight_array" format="reference" />
    <attr name="rowCount" format="integer" />
    <attr name="columnCount" format="integer" />
    <attr name="dividerColor" format="color" />
    <attr name="dividerWidth" format="dimension" />
  </declare-styleable>

  <declare-styleable name="FormLayout_Layout">
    <attr name="layout_rowIndex" format="integer" />
    <attr name="layout_rowCount" format="integer" />
    <attr name="layout_columnIndex" format="integer" />
    <attr name="layout_columnCount" format="integer" />
  </declare-styleable>

  <declare-styleable name="ScrollTv">
    <attr name="receive_move_event" format="boolean" />
  </declare-styleable>
  <declare-styleable name="KeyBoardLayout">
    <attr name="vertical_divide_space" format="dimension" />
    <attr name="horizontal_divide_space" format="dimension" />
    <attr name="divide_color" format="color" />
    <attr name="column_count" format="integer" />
    <attr name="row_count" format="integer" />
    <attr name="width" format="dimension" />
    <attr name="height" format="dimension" />
  </declare-styleable>
  <declare-styleable name="KeyBoardLayout_Child">
    <attr name="rowIndex" format="integer" />
    <attr name="columnIndex" format="integer" />
    <attr name="vertical_span" format="integer" />
    <attr name="horizontal_span" format="integer" />

  </declare-styleable>

  <!--<declare-styleable name="CircleImageView">-->
    <!--<attr name="civ_border_width" format="dimension" />-->
    <!--<attr name="civ_border_color" format="color" />-->
    <!--<attr name="civ_border_overlay" format="boolean" />-->
    <!--<attr name="civ_fill_color" format="color" />-->
  <!--</declare-styleable>-->

  <declare-styleable name="CircleImageView">
    <attr name="civ_border_width" format="dimension" />
    <attr name="civ_border_color" format="color" />
    <attr name="civ_border_overlay" format="boolean" />
    <attr name="civ_fill_color" format="color" />
    <attr name="civ_circle_background_color" format="color" />
  </declare-styleable>

  <declare-styleable name="fading_text_view">
    <attr name="texts1" format="string" />
    <attr name="timeout" format="integer" />
  </declare-styleable>
</resources>